{
  "parser": "babel-eslint",
  "plugins": [
    "babel"
  ],
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "object-shorthand": 2,
    "prefer-spread": 2,
    "func-names": 0,
    "camelcase": [2, {"properties": "always"}],
    "dot-notation": [2, {"allowPattern": "^[a-z]+(_[a-z]+)+$"}],
    "no-param-reassign": 0,
    "new-cap": 1,
    "no-use-before-define": [2, "nofunc"],
    "babel/new-cap": 2,
    "react/jsx-closing-bracket-location": 0,
    "react/prefer-es6-class": 2
  }
}
