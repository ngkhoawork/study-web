  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "npm run -s build-server && BABEL_ENV=webpack webpack --config webpack.build.js --bail -p",
    "build-dev": "NODE_ENV=development npm run -s build",
    "build-server": "BABEL_ENV=server babel app/server -d dist/server",
    "dev": "API_HOST=http://localhost:3000 BABEL_ENV=webpack NODE_ENV=development webpack-dev-server --history-api-fallback --inline --progress --devtool eval --colors",
    "lint": "eslint .",
    "postinstall": "npm run -s build",
    "start-server": "node dist/server/index.js",
    "test": "BABEL_ENV=webpack NODE_ENV=development karma start --single-run",
    "test:live": "NODE_ENV=development karma start --auto-watch --no-single-run",
    "start": "nf start dev",
    "server": "nf start web"
  },
